<configuration>
	<system.web>
		<compilation debug="true" targetFramework="4.0"/>
		<httpRuntime targetFramework="4.8.1"/>
		<authentication mode="Forms"/>
		<profile enabled="true">
			<properties>
				<add name="Fname"/>
				<add name="Lname"/>
				<add name="favcolor"/>
				<add name="profilepic"/>
			</properties>
		</profile>
	</system.web>
	<appSettings>
		<add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
	</appSettings>
</configuration>
